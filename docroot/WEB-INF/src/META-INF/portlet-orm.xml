<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.telefonica.model.impl.DepartmentModelImpl">
		<attributes>
			<id name="idDept" />
			<basic name="nameDept" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.telefonica.model.impl.EmployerModelImpl">
		<attributes>
			<id name="idEmployer" />
			<basic name="nameEmployer" />
			<basic name="idDept" />
			<basic name="photo" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<entity class="com.telefonica.model.impl.DepartmentImpl" name="Department">
		<table name="manageEmployer_Department" />
		<attributes />
	</entity>
	<entity class="com.telefonica.model.impl.EmployerImpl" name="Employer">
		<table name="manageEmployer_Employer" />
		<attributes />
	</entity>
</entity-mappings>